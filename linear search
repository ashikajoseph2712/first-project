#include<stdio.h>
int main()
{
    int ar[100],n,key,i;
    printf("Enter the number of elements\n");
    scanf("%d",&n);
    printf("Enter the elements:\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&ar[i]);
    }
    printf("Enter the element to be searched\n");
    scanf("%d",&key);
    for(i=0;i<n;i++)
    {
        if(ar[i]==key)
        {
            printf("The element %d is found at position %d \n",key,i+1);
            return 0;
        }
    }
    printf("The element is not found");
    return 0;
}
